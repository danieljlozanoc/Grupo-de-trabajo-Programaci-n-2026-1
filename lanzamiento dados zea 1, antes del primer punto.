# codigo lanzamiento de dados nicolas zea 1(corregido)
import random
def total_dados():
    pri_dado=random.randint(1,6)
    print(f"primer dado= {pri_dado}")
    seg_dado=random.randint(1,6)
    print(f"segundo dado= {seg_dado}")
    print(f"la suma de los dados es= {pri_dado + seg_dado}")
    return pri_dado, seg_dado
pri_dado, seg_dado= total_dados()
