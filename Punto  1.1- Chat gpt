import random

# Solicitar nombre del jugador
nombre = input("Ingrese el nombre del jugador: ")

# Lanzamiento de los dos dados
dado1 = random.randint(1, 6)
dado2 = random.randint(1, 6)

print(f"Resultado de los dados: {dado1} y {dado2}")

# Verificar si son "presas" (es decir, n√∫meros iguales)
if dado1 == dado2:
    print(f"Felicitaciones {nombre} Puedes sacar una ficha")
else:
    print("Turno del siguiente jugador")
